client
remote $domain
port 443
proto tcp

auth SHA512
auth-nocache
cipher AES-256-GCM
dev tun
keepalive 10 120
key-direction 1
mssfix 1450
nobind
persist-key
persist-tun
pull
push "redirect-gateway def1"
redirect-gateway def1
remote-cert-tls server
reneg-sec 0
resolv-retry infinite
tun-mtu 1500
verb 0
